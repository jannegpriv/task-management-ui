{
  "name": "task-management-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . -c .eslintrc.cjs --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
